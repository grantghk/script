local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Grant hub-Select script", Ocean)
local NDS = Window:NewTab("NDS")
local BW = Window:NewTab("BedWars")
local KL = Window:NewTab("King-Legacy")
local PL = Window:NewTab("Prinson Life")
local NJ = Window:NewTab("Ninja Legend")
local NDSS = NDS:NewSection("Natural-Disaster-Survival")
local BWS = BW:NewSection("BedWars")
local KLS = KL:NewSection("King-Legacy")
local PLS = PL:NewSection("Prison-Life")
local NJS = NJ:NewSection("Ninja legend")

NDSS:NewButton("NDS", "get script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/grantghk/script/main/NDS"))()
end)
BWS:NewButton("BedWar", "get script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/grantghk/script/main/BW"))()
end)
KLS:NewButton("King-legacy", "get script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/grantghk/script/main/king-legacy"))()
end)
PLS:NewButton("Prison-Life", "get script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/grantghk/script/main/Prison%20life"))()
end)
NJS:NewButton("Prison-Life", "get script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/grantghk/script/main/ninja"))()
end)
